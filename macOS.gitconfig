[user]
	#name = Erik Nordstr√∏m
	#email = erik@nordstroem.no
[help]
	autocorrect = prompt
[http]
	#sslCAInfo = /Users/erikn/myCA6.pem
[credential]
	#helper = osxkeychain
[commit]
	verbose = true
[init]
	defaultBranch = main
[filter "lfs"]
	required = true
	clean = git-lfs clean -- %f
	smudge = git-lfs smudge -- %f
	process = git-lfs filter-process
[branch]
	sort = -committerdate
[tag]
	sort = version:refname
[fetch]
	prune = true
	pruneTags = true
	all = true
[pull]
	ff = only
[merge]
	conflictStyle = zdiff3
[diff]
	algorithm = histogram
	colorMoved = plain
	mnemonicPrefix = true
	renames = true
	#tool = difftastic
[core]
	#pager = delta
[interactive]
	#diffFilter = delta
[rebase]
	autoSquash = true
	autoStash = true
	updateRefs = true
[rerere]
	#enabled = true
	#autoupdate = true
[push]
	default = simple
	autoSetupRemote = true
	followTags = true
